{
  "name": "react-hook-dialog",
  "version": "1.2.0",
  "description": "React hooks for master your dialog(modal) component",
  "sideEffects": false,
  "source": "src/index.ts",
  "files": [
    "dist"
  ],
  "main": "dist/react-hook-dialog.js",
  "module": "dist/react-hook-dialog.module.js",
  "unpkg": "dist/react-hook-dialog.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm run build",
    "build": "rimraf dist && microbundle -f es,umd,cjs --globals react/jsx-runtime=jsx",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "lint": "eslint \"**/*.{js,ts,tsx}\" --fix"
  },
  "keywords": [
    "react",
    "hook",
    "dialog",
    "modal"
  ],
  "repository": "git@github.com:jsun969/react-hook-dialog.git",
  "author": "Justin Sun",
  "license": "MIT",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lint-staged": "^13.1.1",
    "microbundle": "^0.15.1",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18"
  },
  "lint-staged": {
    "*.{js,ts,tsx,md}": [
      "prettier --write"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}