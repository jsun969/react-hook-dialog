{
  "name": "react-hook-dialog",
  "version": "1.1.1",
  "description": "React hook for master your dialog component",
  "sideEffects": false,
  "source": "src/index.ts",
  "files": [
    "dist"
  ],
  "main": "dist/react-hook-dialog.js",
  "module": "dist/react-hook-dialog.module.js",
  "unpkg": "dist/react-hook-dialog.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm run build",
    "build": "rimraf dist && microbundle -f es,umd,cjs --globals react/jsx-runtime=jsx",
    "test": "jest",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint \"**/*.{js,ts,tsx}\" --fix"
  },
  "keywords": [
    "react",
    "hook",
    "dialog",
    "modal"
  ],
  "repository": "git@github.com:jsun969/react-hook-dialog.git",
  "author": "Justin Sun",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "microbundle": "^0.15.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.0",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  }
}